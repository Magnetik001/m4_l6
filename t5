vowel_letters = ['а','е','ё','и','о','у','ы','э','ю','я']

letters = ['а','б','в','г','д','е','ё','ж','з','и','й','к','л','м',\
'н','о','п','р','с','т','у','ф','х','ц','ч','ш','щ','ъ','ы','ь','э','ю','я',]

pos = 0
for i in letters:
    pos += 1 
    print('-'*10)
    print(f'[{i.upper()}] [{i}]')
    print(f'{pos}-я буква алфавита')
    print('Гласная') if i in vowel_letters else print('Согласная')
    print('-'*10)
